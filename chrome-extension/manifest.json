{
  "name": "engram Web Clipper",
  "description": "Send any website to engram",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Send to engram",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "permissions": ["tabs"],
  "host_permissions": ["https://engram.xyzdigital.com/"]
}
