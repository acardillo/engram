{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
  {
    "name": "(lldb) Launch",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/paper/paper",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${fileDirname}",
    "environment": [],
    "externalConsole": false,
    "MIMode": "lldb"
  },
  {
    "name": "Launch record",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/record/main",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}/record",
    "environment": [],
    "externalConsole": false,
    "MIMode": "lldb"
  },
    {
      "name": "(lldb) Launch ceg",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/planner/clients/cli/eg/c/ceg",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/planner/clients/cli/eg/c",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/fe"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/be/index.js",
      "cwd": "${workspaceFolder}/be"
    },
    {
      "name": "Debug CRA Tests",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/fe/node_modules/.bin/react-scripts",
      "args": [
        "test",
        "--runInBand",
        "--no-cache",
        "--watchAll=false"
      ],
      "cwd": "${workspaceRoot}/fe",
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "CI": "true"
      },
      "disableOptimisticBPs": true
    }
  ]
}